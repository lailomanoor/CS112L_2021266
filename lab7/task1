#include <iostream>
#include <string>
using namespace std;

class Person
{
    string username;
    int ID;
    static int count;
   // Date dateOfJoining;

    public: 

    Person()
    {
        username = " ";
        ID = 0;
        count++;
    }

     friend istream& operator>>(istream& input, Person &p)
    {
       // cout << "enter name and ID\n";
        input >> p.username >> p.ID;

       // cout << "enter date in the format yyyy mm dd\n";
       // input >> p.dateOfJoining;

        return input;
    }

    friend ostream& operator<<(ostream& output, Person p)
    {
        output << "username: " << p.username << " ID: " << p.ID; 

        return output;
    }

    
    friend class Date;
};

class Date
{
    int month, date, year;
    Person person;

    public:
   // friend class Person;

    Date()
    {
        month = 1;
        date = 1;
        year = 1;
    }

    friend ostream& operator<<(ostream &output, Date d)
    {
            output << d.date << " - " << d.month << " - " << d.year << endl;

        return output;
    }

    friend istream& operator>>(istream &input , Date &d)
    {
        cout << "enter name and ID\n";
        input >> d.person;

        cout << "enter date in format yyyy mm dd\n";
        bool valid = false;

        while (!valid)
        {
            input >> d.year >> d.month >> d.date;
            if (d.date < 31 && d.date > 0 && d.month < 12 && d.month >0 && d.year > 0)
                valid = true;
            else
                cout << "invalid date. try again\n";
        }



        return input;
    }

    bool operator<(Date d)
    {
        if (year < d.year)
            return true;
        if (month < d.month)
            return true;
        if (date < d.date)
            return true;

        return false;
    }

    bool operator==(Date d)
    {
        if (year == d.year && month == d.month && date == d.date)
            return true;

        return false;
    }

    void display( Date d)
    {   
        if (date < d.date)
            cout << d.person.username << " is senior to " << person.username << " because " << *this << " is earlier than " << d<< endl;
        else if (date < d.date)
            cout << d.person.username << " and " << person.username << " joined on the same day";
        else
            cout << person.username  << " is senior to " << d.person.username << " because " << d << " is earlier than " << *this << endl;
    }

};



int Person::count = 0;

int main()
{
    Person p;
    cin >> p;
    Date d1,d2;
    cin >> d1;
    cin >> d2;
    d1.display(d2);
    return 0;
}
